        int min = Integer.MAX_VALUE;
        int max = Integer.MIN_VALUE;
        long total = 0;
        for (int i = 0; i < arr.size(); i++) {
            total += arr.get(i);
            if (arr.get(i) > max) {
                max = arr.get(i);
            }
            if (arr.get(i) < min) {
                min = arr.get(i);
            }
        }
        System.out.println((total - max) + " " + (total - min));
    
